algoritmo "ex8-escreve-caractere-mostra-antecessor"

var
base, altura :real

inicio
      escreval("Digite o valor da BASE do retângulo em cm: ")
      leia(base)
      
      escreval("Digite o valor da ALTURA do retângulo em cm: ")
      leia(altura)

      escreval("A área deste retângulo é de", base * altura, " cm")
fimalgoritmo
